{
  "name": "@redactie/translations-module",
  "version": "1.4.0",
  "description": "redactie translations module",
  "main": "dist/redactie-translations-module.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && webpack --env.prod",
    "build:w": "npm run build:types && webpack -w --env.prod",
    "build:demo": "npm run build:types && webpack -w --env.dev",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "trans": "npm run trans:extract && npm run trans:jsonToPo",
    "trans:extract": "./node_modules/.bin/i18next-scanner --config i18next-scanner.config.js",
    "trans:jsonToPo": "node ./scripts/jsonToPoAndPot.js",
    "start": "concurrently --kill-others \"npm run build:demo\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public",
    "docs": "npm run docs:html && npm run docs:markdown",
    "docs:html": "typedoc --plugin typedoc-plugin-missing-exports --excludeExternals true --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/api.ts --excludePrivate --out docs",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme --plugin typedoc-plugin-missing-exports --excludeExternals true --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/api.ts --excludePrivate --theme github-wiki --out docs/markdown"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.antwerpen.be/projects/REDA/repos/redactie-translations_module_react"
  },
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "keywords": [
    "Redactie",
    "React",
    "Translations",
    "Module"
  ],
  "devDependencies": {
    "@acpaas-ui/react-components": "4.0.0",
    "@acpaas-ui/react-editorial-components": "0.0.1-6",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@datorama/akita": "4.22.0",
    "@redactie/module-webpack-plugin": "2.0.0",
    "@redactie/redactie-core": "1.11.0",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/i18next": "^13.0.0",
    "@types/jest": "24.9.1",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.kebabcase": "4.1.6",
    "@types/query-string": "^6.3.0",
    "@types/ramda": "0.26.44",
    "@types/react": "16.9.19",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router-dom": "5.1.3",
    "@types/webpack-env": "1.15.1",
    "@types/yup": "0.26.33",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "babel-jest": "25.1.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.1.0",
    "css-loader": "3.4.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "3.1.0",
    "i18next-conv": "^10.0.1",
    "i18next-scanner": "^2.11.0",
    "i18next-scanner-typescript": "^1.0.3",
    "install": "0.13.0",
    "jest": "25.1.0",
    "ky": "0.17.0",
    "node-sass": "4.13.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "ramda": "0.27.0",
    "react": "16.12.0",
    "react-router-dom": "5.1.2",
    "rxjs": "6.3.3",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "typedoc": "^0.22.15",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "4.6.3",
    "typescript-plugin-css-modules": "2.1.2",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "@wcm/paging-package-nodejs": "^2.0.6",
    "i18next": "^19.4.4",
    "ky": "0.17.0",
    "query-string": "6.12.1",
    "ramda": "0.27.0",
    "react-i18next": "^11.4.0"
  },
  "author": "Jeroen Valcke",
  "license": "ISC"
}
